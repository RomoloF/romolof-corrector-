{
  "manifest_version": 3,
  "name": "RomoloF Writer Assistant",
  "version": "1.1",
  "icons": {
    "128": "icon.png"
  },
  "action": {
    "default_icon": "icon.png",
    "default_title": "RomoloF Writer"
  },
  "permissions": [
    "contextMenus",
    "activeTab",
    "scripting",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
